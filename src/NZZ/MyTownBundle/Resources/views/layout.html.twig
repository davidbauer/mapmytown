{% extends "::base.html.twig" %}
{% block stylesheets %}
    {% stylesheets filter='?yui_css' output='css/*.css'
    'bundles/nzzmytown/vendor/bootstrap/css/bootstrap.min.css'
    'bundles/nzzmytown/vendor/bootstrap/css/bootstrap-responsive.css'
    'bundles/nzzmytown/css/base/layout.css'
    'bundles/nzzmytown/css/base/typography.css'
    'bundles/nzzmytown/css/components/entry.css'
    'bundles/nzzmytown/css/components/introduction.css'
    'bundles/nzzmytown/css/components/map.css'
    'bundles/nzzmytown/css/app.css'
    %}
    <link rel="stylesheet" type="text/css" href="{{ asset_url }}" />
    {% endstylesheets %}
    <link rel="stylesheet" href="http://cdn.leafletjs.com/leaflet-0.6.4/leaflet.css" />
    <!--[if lte IE 8]>
    <link rel="stylesheet" href="http://cdn.leafletjs.com/leaflet-0.6.4/leaflet.ie.css" />
    <![endif]-->
 {% endblock %}

{% block javascripts %}
    {% javascripts filter='?yui_js' output='js/*.js'
          'bundles/nzzmytown/vendor/bootstrap/js/bootstrap.min.js'
          'bundles/nzzmytown/vendor/underscore/underscore-min.js'
          'bundles/nzzmytown/vendor/backbone/backbone.js'
          'bundles/nzzmytown/js/app.js'
          'bundles/nzzmytown/js/views/RootView.js'
    %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}
